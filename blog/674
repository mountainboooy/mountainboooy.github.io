  $('.hero__btn').click(toggleMenu)    
  $('.drawer').click(toggleMenu)
})

function toggleMenu () {
  if (isShown()) {
    closeMenu()
  } else {
    openMenu()
  }
}

function isShown () {
  return $('.drawer').css('display') === 'block'
}

function openMenu () {
  $('.drawer').show()
  setTimeout(function () {
    $('.backdrop').addClass('open')
    $('.drawer ul').addClass('open')
  }, 100)
}

function closeMenu () {
  $('.drawer ul').removeClass('open')
  $('.backdrop').removeClass('open')
  setTimeout(function () {
    $('.drawer').hide()
  }, 300)
}</script></div></body></html>